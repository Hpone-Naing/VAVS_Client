@model VAVS_Client.Models.TaxValidation
@{
    Layout = "_Layout";
}

@{
    ViewData["Title"] = "Detail";
}

<div id="spinner-container">
    <img id="spinner" decoding="async" src="~/image/loading/spinner.gif" asp-append-version="true" />
</div>


<div style="background-color: #0f3cbd;;" class="row mt-3 border border-dark">
    <div class="d-flex justify-content-center align-item-center p-1">
        <label class="text-white"> အသေးစိတ် </label>
    </div>
</div>

<div class="row mt-1 border border-dark" style="border-radius: 20px;background-color: #d7eafe78;">
    <div class="col-lg-6 col-md-6 col-sm-12">
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">NRC</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.PersonNRC
            </div>
        </div>
        <div class="row p-3">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Township</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.Township.TownshipName
            </div>
        </div>
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">State Division</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.Township.StateDivision.StateDivisionName
            </div>
        </div>
        <div class="row p-3">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">ကားနံပါတ်</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.VehicleNumber
            </div>
        </div>
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Manufacturer</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.Manufacturer
            </div>
        </div>
        <div class="row p-3">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Country Made</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.CountryOfMade
            </div>
        </div>
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">VehicleBrand</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.VehicleBrand
            </div>
        </div>
        <div class="row p-3">
            <label class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">BuildType</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.BuildType
            </div>
        </div>
        
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12">
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Model Year</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.ModelYear
            </div>
        </div>
        <div class="row p-3">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Engine Power</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.EnginePower
            </div>
        </div>
        
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Fuel Type</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.FuelType
            </div>
        </div>
        <div class="row p-3">
            <label class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Standard Value</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.StandardValue
            </div>
        </div>
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Contract Price</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.ContractValue
            </div>
        </div>
        <div class="row p-3">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Tax Amount</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.TaxAmount
            </div>
        </div>
        <div class="row p-3" style="background-color: #c0c0c052;">
            <label  class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">QR CodeNumber</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.QRCodeNumber
            </div>
        </div>
        <div class="row p-3">
            <label class="fw-bold col-lg-6 col-md-6 col-sm-4 control-label">Demand Number</label>
            <div class="col-lg-6 col-md-6 col-sm-8">
                @Model.DemandNumber
            </div>
        </div>
    </div>
</div>

